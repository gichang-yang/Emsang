<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wala.yang_gichang.emsang">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="엠생"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="wala.wala.yang_gichang.emsang.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="wala.wala.yang_gichang.emsang.MenuActivity"
            android:theme="@style/AppTheme"
            android:screenOrientation="portrait"/>
        <activity android:name="wala.wala.yang_gichang.emsang.GamePlay" />
        <activity android:name="wala.wala.yang_gichang.emsang.GameIntroActivity" />
        <activity android:name="wala.wala.yang_gichang.emsang.Choice" />
        <activity android:name="wala.wala.yang_gichang.emsang.ChoicePlayActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="wala.wala.yang_gichang.emsang.Search"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name="wala.wala.yang_gichang.emsang.Profile"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>